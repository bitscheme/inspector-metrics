{
  "name": "inspector-metrics",
  "version": "1.5.0",
  "main": "./build/lib/metrics/index.js",
  "typings": "./build/lib/metrics/index.d.ts",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://www.opensource.org/licenses/mit-license.php"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rstiller/inspector-metrics"
  },
  "keywords": [
    "metrics",
    "typescript"
  ],
  "devDependencies": {
    "@types/async": "^2.0",
    "@types/chai": "^3.4",
    "@types/mocha": "^2.2",
    "@types/node": "^7.0",
    "@types/sinon": "^2.1",
    "@types/sinon-chai": "^2.7",
    "chai": "^4.0.0",
    "mocha": "^3.4.2",
    "mocha-typescript": "^1.1.4",
    "npm-check": "^5.4.4",
    "nyc": "^10.2",
    "reflect-metadata": "^0.1",
    "rimraf": "^2.6.1",
    "sinon": "^2.3.2",
    "sinon-chai": "^2.9",
    "ts-node": "^3.0",
    "tslint": "^5.3.2",
    "typescript": "^2.3.3"
  },
  "dependencies": {
    "@types/node": "^7.0.29",
    "@types/sinon": "^2.3.1",
    "@types/sinon-chai": "^2.7.28",
    "async": "^2.4.1",
    "chai": "^4.0.2",
    "sinon": "^2.3.4",
    "source-map-support": "^0.4",
    "ts-node": "^3.0.6",
    "tslint": "^5.4.3",
    "typescript": "^2.3.4"
  },
  "files": [
    "build",
    "lib",
    "test",
    "mocha.opts",
    "tsconfig.json",
    "tslint.json"
  ],
  "scripts": {
    "check:lint": "tslint -c tslint.json lib/**/*.ts test/**/*.ts",
    "check:coverage": "nyc npm run test:jenkins",
    "check:deps": "npm-check --ignore @types/* --no-spinner --no-emoji --no-color",
    "clean": "rimraf build coverage",
    "compile": "tsc -p .",
    "build": "npm run clean && npm run compile",
    "prepublish": "npm run build",
    "test": "mocha --opts mocha.opts",
    "test:jenkins": "mocha --opts mocha.jenkins.opts"
  },
  "nyc": {
    "include": [
      "lib/*.ts",
      "lib/**/*.ts"
    ],
    "exclude": [
      "test/*"
    ],
    "extension": [
      ".ts"
    ],
    "require": [],
    "reporter": [
      "json",
      "html",
      "cobertura"
    ],
    "all": true
  }
}
